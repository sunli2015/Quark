<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-3.2.xsd" default-autowire="byName">
    <bean id="init" class="cn.org.coral.core.init.InitManager" init-method="init">
    	<property name="location" value="classpath:/init-data"/>
    	<property name="schema" value="${db.default_schema}"/>
    </bean>
</beans>